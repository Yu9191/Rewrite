[Rewrite]
# 广告 
^https://[a-zA-Z0-9-]+\.cloudfront\.net/api/(?:sys/partner/list|activity/indexActs|sys/advertisement/list|sys/getImgAndVideoCdnList|aibox/entranceConfig) - reject

[Script]
# 视频
http-request https:\/\/[^\/]+\.cloudfront\.net\/api\/m3u8\/decode\/authPath script-path=https://raw.githubusercontent.com/Yu9191/Rewrite/refs/heads/main/ydm2.js, timeout=60, tag=ydm2

# 会员
http-response ^https://[a-zA-Z0-9-]+\.cloudfront\.net/api/(video/getVideoById|user/base/info|community/dynamic/dynamicInfo) script-path=https://raw.githubusercontent.com/Yu9191/Rewrite/refs/heads/main/ydm.js, requires-body=true, timeout=60, tag=ydm

[MITM]
hostname = *.cloudfront.net